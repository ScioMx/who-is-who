<%= form_for @user do |f| %>
  <p>
    <%= f.label :name %> <br>
    <%= f.text_field :name %> <br>
  </p>

  <p>
    <%= f.label :email %> <br>
    <%= f.text_field :emails %>
  </p>

   <p>
      <%= f.label :birthplace %> <br>
      <%= f.text_field :birthplace %>
    </p>

    <p>
      <%= f.label :interest %> <br>
      <%= f.text_field :interests %>
    </p>
  
  <p>
    <%= f.label :responsibilities %> <br>
    <%= f.text_area :responsibilities %>
  </p>

  <p>
    <%= f.file_field :avatar %> <br>
    <br>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
